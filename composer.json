{
    "name": "tray/php-client",
    "description": "Api client",
    "type": "library",
    "authors": [
        {
            "name": "Rodrigo Damasceno",
            "email": "rodrigo.damasceno@tray.net.br"
        }
    ],
    "require": {
        "php": "^7.3|^8.0",
        "ext-json": "*",
        "doctrine/inflector": "^2.0",
        "guzzlehttp/guzzle": "^7.4",
        "nesbot/carbon": "^2.55",
        "voku/portable-ascii": "^1.5"
    },
    "require-dev": {
        "fakerphp/faker": "^1.9.1",
        "mockery/mockery": "^1.3.1",
        "phpmd/phpmd": "^2.11",
        "phpro/grumphp": "^1.5",
        "phpstan/phpstan": "^1.2",
        "phpunit/phpunit": "^9.5.10",
        "squizlabs/php_codesniffer": "^3.6",
        "symfony/dotenv": "^5.4"
    },
    "autoload": {
        "psr-4": {
            "Tray\\": "src/Tray"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Tests\\": "tests/"
        }
    },
    "config": {
        "preferred-install": "dist",
        "sort-packages": true,
        "optimize-autoloader": true,
        "allow-plugins": {
            "phpro/grumphp": true
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
